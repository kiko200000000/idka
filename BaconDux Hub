local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("BaconDux Hub", "BloodTheme")

local Tab = Window:NewTab("Games")
local PrisionLifeSection = Tab:NewSection("Prision Life")


local MM2Section = Tab:NewSection("Murder Mistery 2")


local MadCitySection = Tab:NewSection("Mad City")



local Tab = Window:NewTab("Credits")


local CreditsSection = Tab:NewSection("Credits")
CreditsSection:NewLabel("Credits: Bendza")
























